package org.vaibhao;
class ConvertToUpper{
	char ch[];
	void setVharArray(char c[]) {
		System.out.println("Before conversion is");
		for(int i=0; i<ch.length; i++) {
			System.out.printf("ch[%d]-->%c\n",i,ch[i]);
		}
		for(int i=0;i<ch.length;i++) {
			if(ch[i]>='a' && ch[i]<='z') {
				int c=(int)ch[i]-32;
			}
		}
	}
}
